error) => {\n                setLoading(false);\n                toast.error(error.message);\n            });\n    }\n\n    return (\n        <section className='container text-center'>\n            <div className='form'>\n                <h2>Login</h2>\n                <form onSubmit={loginUser}>\n                    <div>\n                        <input type='text' className='m-2' placeholder='Emaili daxil edin.'\n                            required value={email} onChange={(e) => setEmail(e.target.value)} />\n                        <input type='text' className='m-2' placeholder='Şifrəni daxil edin.'\n                            required value={password} onChange={(e) => setPassword(e.target.value)} />\n                    </div>\n                    {loading && <Loader />}\n                    <button  className='btn btn-primary'>Daxil Olun</button>\n                    <div className='links'>\n                        <Link to='/reset'>\n                            Şifrəni Yenilə\n                        </Link>\n                    </div>\n                    <p>-- və ya --</p>\n                </form>\n                <ToastContainer />\n                <button type='submit' onClick={signInWithGoogle} className='btn btn-danger'> Google ilə Daxil Olun</button>\n                <p>\n                    Hesabınız Yoxdur?\n                    <Link to='/register'>Qeydiyyatdan Keçin</Link>\n                </p>\n            </div>\n        </section>\n    )\n}\n\nexport default Login","/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/Auth/Firebase/config.js",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Layouts/Header/Header.js",["214","215","216","217","218","219","220"],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Redux/store.js",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Redux/Slice/AuthSlice.js",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Redux/Slice/aut