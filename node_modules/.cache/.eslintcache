 if (query === \"\") {\n        return product;\n      } else if(product.name.toLowerCase().includes(query.toLowerCase())) {\n        \n        return product;\n      }\n      else  if(product.company.name.toLowerCase().includes(query.toLowerCase())) {\n        \n        return product;\n      }\n      \n      \n    }).map((product)=>\n    <div key={product.id}>\n      <JobCard product={product}/>\n    </div>\n   ).reverse();\n   }\n\n  return (\n    <div>\n     <div className='d-flex container'>\n     <h1  className='text container '  style={{color:'var(--pink)',fontSize:'35px',alignItems:'center',display:'flex'}}>Aktiv Vakansiyalar</h1>\n    <form className='searchJob mb-5' style={{alignItems:'center',display:'flex',top:'20px'}}>\n      <input  className='searchBar mt-2 p-2'  onChange={event =>console.log( setQuery(event.target.value))} type='text'></input>\n      <button className='search__submit' type='submit'>\n                  <img src='https://www.citypng.com/public/uploads/small/116400840186bmo0mkghcdq8hn99wmpbh9wrgbytghidisi7e41vegbclccrb6kg9hrk0ebgqrpc5ezz2tjtkvsfsvk7yfbakowxbpvrzsakde3.png'   alt='some value'/>\n      </button>\n\n    </form>\n     </div>\n\n      {content}\n\n    </div>\n  )\n}\n\nexport default Jobs","/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/AboutPage/Hero/Hero.tsx",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/AboutPage/Image/Image.tsx",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/AboutPage/Contact/Contact.tsx",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/MainPage/Info/index.jsx",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/MainPage/Image/Index.jsx",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/Jobs/Loader.js",[],[],"/Users/imac/Desktop/Jobly/FrontEntJobly/JoblyReact-main/src/Components/Jobs/JobCard.js",[],[],"/Users/imac/Desktop/Jobly/FrontEntJ